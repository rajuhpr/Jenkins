pipeline {
  agent none
  stages {
    stage('Git-checkout') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'echo passed'
      }
    }
    stage('maven build') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'mvn --version'
        sh 'Jenkins-CI-CD/java-project && mvn clean package'
        echo 'maven build works!'
      }
    }
  }
}
